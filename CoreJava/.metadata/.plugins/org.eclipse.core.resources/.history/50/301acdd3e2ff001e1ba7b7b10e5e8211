package strings.hyr;

public class StringsFirst {
	public static void main(String[] args) {
		String s1 = "Pavan";
		String s2 = "Pavan";
		//some operations
		
		 s1 = s1+" Kumar";
		 
		 System.out.println(s1 == s2); 
		 
		 String s3 = "PavanKumar";
		 //if we can able to change means the variables should not be equals
		 
		 
	}

}

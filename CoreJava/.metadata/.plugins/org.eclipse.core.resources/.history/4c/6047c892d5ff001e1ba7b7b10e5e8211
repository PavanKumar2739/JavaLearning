package multi_threading;

public class TestMultiThreading {
	public static void main(String[] args) {
		Runnable r1 = () ->{
			for (int i = 0; i < 1000; i+=50) {
				
			}
			
		};
		Runnable r2 = () ->{
			for (int i = 0; i < 1000; i+=50) {
				
			}
			
		};

		Runnable r3 = () ->{
			for (int i = 0; i < 1000; i+=50) {
				
			}
			
		};

	}

}
class BrickDairy{
	int brickCount = 0;
	
	public void incrementBrickCount() {
		brickCount +=50;
	}
}
